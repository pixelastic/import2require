{
  "devDependencies": {
    "aberlaas": "pixelastic/aberlaas",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0"
  },
  "description": "Convert `import foo from 'foo'` to `const foo = require('foo')` from the commandline",
  "bin": {
    "import2require": "bin/import2require"
  },
  "files": [
    "bin/*",
    "lib/*.js"
  ],
  "main": "lib/main.js",
  "name": "import2require",
  "scripts": {
    "ci": "./scripts/ci",
    "husky:precommit": "./scripts/husky-precommit",
    "husky:prepare-commit-msg": "./scripts/husky-prepare-commit-msg",
    "lint": "./scripts/lint",
    "lint:fix": "./scripts/lint-fix",
    "release": "./scripts/release",
    "test": "./scripts/test",
    "test:watch": "./scripts/test-watch"
  },
  "version": "0.1.3",
  "dependencies": {
    "firost": "1.5.1",
    "golgoth": "1.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
